#include<iostream>
#include<string>

using namespace std;

class Solution
{
public:
	int romanToInt(string s) 
	{
		int sum = 0,lens = s.size();

		for (int i = 0;i < lens; i++)
		{
			if (s[i] == 'I') 
			{
				if (i < lens - 1 && s[i + 1] == 'V') sum += -1;			
				else if (i < lens - 1 && s[i + 1] == 'X') sum += -1;
				else sum += 1; 
			}
			else if (s[i] == 'V') sum += 5;
			else if (s[i] == 'X')
			{
				if (i < lens - 1 && s[i + 1] == 'L') sum += -10;
				else if (i < lens - 1 && s[i + 1] == 'C') sum += -10;
				else sum += 10;
			}
			else if (s[i] == 'L') sum += 50;
			else if (s[i] == 'C') 
			{ 
				if (i < lens - 1 && s[i + 1] == 'D') sum += -100;
				else if (i < lens - 1 && s[i + 1] == 'M') sum += -100;
				else sum += 100;
			}
			else if (s[i] == 'D') sum += 500;
			else if (s[i] == 'M') sum += 1000;
		}
		return sum;








	}
};


int main() 
{
	// 输入在 1~3999
	Solution s;
	
	cout << s.romanToInt("MCMXCIV") << endl;
	return 0;
}
