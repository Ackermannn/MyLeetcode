#include<iostream>
#include<vector>
using namespace std;
class Solution {
public:
	string longestCommonPrefix(vector<string>& strs) {
		
		if (strs.empty()) return "";
		string out = strs[0];
		for (int j = 1; j < strs.size(); j++) {
			if (strs[j].size() < out.size()) out = out.substr(0, strs[j].size());
			for (int i = 0; i < out.size(); i++) {
				if (out[i] != strs[j][i])
				{
					out = out.substr(0, i);
					break;
				}
			}
			
		}
		return out;
	}
};


int main() {
	vector<string> strs = { "flpwer" ,"flow","flight" };
	string out;
	Solution s;
	out = s.longestCommonPrefix(strs);
	cout << out << endl;
	
	return 0;
}
